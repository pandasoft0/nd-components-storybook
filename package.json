{
  "name": "neon-district",
  "description": "Neon District Design System",
  "version": "1.1.3",
  "author": "[Vinicius Vicentini, Miguel Gomes]",
  "license": "MIT",
  "main": "index.ts",
  "private": "true",
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "tsup --dts --sourcemap --format esm && rimraf dist",
    "build-storybook": "build-storybook -o ./dist",
    "start": "http-server ./dist",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "0.1.1",
    "@radix-ui/react-label": "0.1.0",
    "@radix-ui/react-radio-group": "0.1.1",
    "@radix-ui/react-toggle": "0.1.1",
    "@radix-ui/react-tooltip": "0.1.1",
    "@stitches/react": "^1.2.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-slick": "^0.28.1",
    "react-textarea-autosize": "^8.3.3",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-slick": "^0.23.7",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-loader": "^8.2.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-server": "^13.0.2",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "prettier": "2.4.1",
    "react-icons": "^4.3.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.6.0",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn build"
    ]
  }
}
